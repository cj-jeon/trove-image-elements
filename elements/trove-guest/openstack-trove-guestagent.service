[Unit]
Description=OpenStack Trove guestagent Service
After=syslog.target network.target cloud-init.service

[Service]
Type=simple
User=trove
ExecStart=/usr/bin/trove-guestagent --config-file /usr/share/trove/trove-dist.conf --config-file /etc/trove/trove-guestagent.conf --config-file /etc/$

[Install]
WantedBy=multi-user.target
